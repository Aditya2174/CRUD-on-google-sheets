const axios = require('axios');

exports.handler = async function (event, context) {
  try {
    const response = await axios.get('http://localhost:8081/sheets'); // Replace YOUR_BACKEND_URL with the URL of your deployed backend
    const data = response.data;
    return {
      statusCode: 200,
      body: JSON.stringify(data),
    };
  } catch (error) {
    return {
      statusCode: 500,
      body: JSON.stringify({ error: 'Internal Server Error' }),
    };
  }
};
